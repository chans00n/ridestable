{
  "version": 2,
  "buildCommand": "npm run prisma:generate",
  "outputDirectory": ".",
  "functions": {
    "api/index.ts": {
      "maxDuration": 60,
      "includeFiles": "src/**,prisma/**"
    },
    "api/db-test.ts": {
      "maxDuration": 30
    },
    "api/connection-test.ts": {
      "maxDuration": 30
    },
    "api/dns-test.ts": {
      "maxDuration": 30
    },
    "api/register-test.ts": {
      "maxDuration": 30
    },
    "api/schema-test.ts": {
      "maxDuration": 30
    },
    "api/webhook-test.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/db-test",
      "destination": "/api/db-test"
    },
    {
      "source": "/connection-test",
      "destination": "/api/connection-test"
    },
    {
      "source": "/dns-test",
      "destination": "/api/dns-test"
    },
    {
      "source": "/register-test",
      "destination": "/api/register-test"
    },
    {
      "source": "/schema-test",
      "destination": "/api/schema-test"
    },
    {
      "source": "/webhook-test",
      "destination": "/api/webhook-test"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}