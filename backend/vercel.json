{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".",
  "functions": {
    "api/index.ts": {
      "maxDuration": 60
    },
    "api/test.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}