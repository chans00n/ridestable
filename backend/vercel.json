{
  "buildCommand": "npm install && npx prisma generate && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --production=false",
  "framework": null,
  "regions": ["iad1"],
  "functions": {
    "api/index.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}